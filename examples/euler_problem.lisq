(defun remainder (devidend divisor) 
  (loop while (>= devidend divisor) do
    (set devidend (- devidend divisor))
  )
  devidend
)

(let ((a 0) (result false) (b 0))
    (loop while (= result false) do 
        (set a (+ a 1))
        (set result true)
        (set b 1)
        (loop while (<= b 20) do 
            (if (!= (remainder a b) 0)
                (set result false)
            )
            (set b (+ b 1))
        )
    )
    (put_char a)
)