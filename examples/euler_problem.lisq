#include lisq_lib/math.lisq
#include lisq_lib/strings.lisq

(defun check (x)
    (let ((result true) (i 20))
        (loop while (& result (>= i 10)) do
            (if (!= (remainder x i) 0)
                (set result false)
            )
            (set i (- i 1))
        )
        result
    )
)

(let ((i (- 232792560 2520)) (result false))
    (loop while (!= result true) do
        (set i (+ i 2520))
        (set result (check i))
    )
    (print_int i)
)